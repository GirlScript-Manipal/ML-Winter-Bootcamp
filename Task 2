import numpy as np

a = np.array([[11,22,33] , [44,55,66]])
print("Input Matrix:")
print(a)
b = np.transpose(a)
print("Transpose  Matrix: ")
print(b)

a = [[111,222,333] , [44,55,66]]
print("Input Matrix:")
for x in a:
    print(x)

b = [[a[j][i] for j in range(len(a))] for i in range(len(a[0]))]
print("\n Transpose Matrix: ")
for y in b:
    print(y)
